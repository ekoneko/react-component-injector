{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "importHelpers": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "lib": ["esnext"]
  }
}
